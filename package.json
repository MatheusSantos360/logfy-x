{
  "name": "logfy-x",
  "version": "1.0.1",
  "description": "",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "configFileName": "logfy-x.json",
  "commandName": "logfy-x",
  "bin": {
    "logfy-x": "./dist/cli/index.js"
  },
  "scripts": {
    "start": "tsx src/playground.ts",
    "cli": "tsx src/cli/index.ts",
    "dev": "tsx watch src/playground.ts",
    "dev:cli": "tsx watch src/cli/index.ts",
    "lint": "eslint",
    "build": "tsup src/index.ts --format cjs,esm --dts && tsup src/cli/index.ts --format cjs --out-dir dist/cli",
    "test": "vitest",
    "test:pkg": "vitest --exclude tests/cli",
    "test:cli": "vitest tests/cli",
    "release": "pnpm run build && changeset publish",
    "verify": "pnpm run start && pnpm run lint && pnpm run test run && pnpm run build && rm -rf ./dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MatheusSantos360/logfy-x.git"
  },
  "keywords": [
    "nodejs",
    "log",
    "logfy",
    "logfy-x",
    "terminal"
  ],
  "author": "Matheus dos Santos Paix√£o <matheusworkcontact@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/MatheusSantos360/logfy-x/issues"
  },
  "homepage": "https://github.com/MatheusSantos360/logfy-x#readme",
  "devDependencies": {
    "@changesets/cli": "^2.27.9",
    "@eslint/js": "^9.13.0",
    "@types/node": "^22.9.0",
    "eslint": "^9.13.0",
    "globals": "^15.11.0",
    "tsup": "^8.3.5",
    "tsx": "^4.19.2",
    "typescript": "^5.6.3",
    "typescript-eslint": "^8.12.2",
    "vitest": "^2.1.4"
  },
  "dependencies": {
    "commander": "^12.1.0",
    "node-emoji": "^2.1.3",
    "picocolors": "^1.1.1",
    "zod": "^3.23.8"
  }
}
